#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/zxc66/Documents/Workspace/demo/实现一个前端脚手架/generator-niconico/node_modules/.store/ejs@3.1.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/zxc66/Documents/Workspace/demo/实现一个前端脚手架/generator-niconico/node_modules/.store/ejs@3.1.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ejs@3.1.8/node_modules/ejs/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ejs@3.1.8/node_modules/ejs/bin/cli.js" "$@"
fi
